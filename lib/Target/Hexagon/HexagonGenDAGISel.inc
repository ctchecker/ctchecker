  return (-31 <= v && v <= -1);

  }
  case 26: { // Predicate_m5BImmPred
    ConstantSDNode*N = cast<ConstantSDNode>(Node);

  // m5BImmPred predicate - True if the (char) number is in range -1 .. -31
  // and will fit in a 5 bit field when made positive, for use in memops.
  // this is specific to the zero extending of a negative by CombineInstr
  int8_t v = (int8_t)N->getSExtValue();
  return (-31 <= v && v <= -1);

  }
  case 27: { // Predicate_m5ImmPred
    ConstantSDNode*N = cast<ConstantSDNode>(Node);

  // m5ImmPred predicate - True if the number is in range -1 .. -31
  // and will fit in a 5 bit field when made positive, for use in memops.
  int64_t v = (int64_t)N->getSExtValue();
  return (-31 <= v && v <= -1);

  }
  case 28: { // Predicate_u5ImmPred
    ConstantSDNode*N = cast<ConstantSDNode>(Node);

  int64_t v = (int64_t)N->getSExtValue();
  return isUInt<5>(v);

  }
  case 29: { // Predicate_u6_2ImmPred
    ConstantSDNode*N = cast<ConstantSDNode>(Node);

  int64_t v = (int64_t)N->getSExtValue();
  return isShiftedUInt<6,2>(v);

  }
  case 30: { // Predicate_u2ImmPred
    ConstantSDNode*N = cast<ConstantSDNode>(Node);

  int64_t v = (int64_t)N->getSExtValue();
  return isUInt<2>(v);

  }
  case 31: { // Predicate_truncstorei32
    SDNode *N = Node;

  return cast<StoreSDNode>(N)->getMemoryVT() == MVT::i32;

