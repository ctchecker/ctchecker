  case 74: { // Predicate_u7ImmPred
    ConstantSDNode*N = cast<ConstantSDNode>(Node);

  int64_t v = (int64_t)N->getSExtValue();
  return isUInt<7>(v);

  }
  case 75: { // Predicate_n8ImmPred
    ConstantSDNode*N = cast<ConstantSDNode>(Node);

  // n8ImmPred predicate - True if the immediate fits in a 8-bit signed
  // field.
  int64_t v = (int64_t)N->getSExtValue();
  return (-255 <= v && v <= 0);

